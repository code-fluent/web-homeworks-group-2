<html>
    <head>
        <title>tema-05medium</title>
        <script>

    var myDate = new Date();
    var hours = myDate.getHours();

               if (hours < 12) {
        console.log("Good morning");
      } else if (hours >= 12 && hours <= 18) {
        console.log("Good afternoon");
      } else if (hours >= 19 && hours <=22)
        {console.log("Good evening");
      }
        </script>
    </head>
</html>